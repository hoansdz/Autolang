// memory_bench.al
val start = Time.now()
class Node<T>(val data: T, var next: Node<T>?) {
}

func benchmarkMemory() {
    var i = 1_000_000
    var list: Node<Int>? = null
    
    while (i > 0) {
        list = Node<Int>(i, list)
        i = i - 1
    }

    println("Created 1M nodes successfully")
}

benchmarkMemory()
println("Total time ${Time.now() - start}")