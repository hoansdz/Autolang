// pi_bench.al
val start = Time.now()
func benchmarkPi() {
    val iterations = 10000000
    var pi = 0.0
    var i = 0
    var sign = 1.0

    while (i < iterations) {
        pi = pi + (sign / (2.0 * i + 1.0))
        sign = sign * -1.0
        i = i + 1
    }
    
    pi = pi * 4.0
    println("Pi: ${pi}")
    println("Done 10M iterations")
}

benchmarkPi()
println("Total time ${Time.now() - start}")